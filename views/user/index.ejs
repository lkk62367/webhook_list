<div class="container">
  <h3>Users</h3>
  <table class="table">
    <tr>
      <th>repositoryName</th>
      <th>repositoryID</th>
      <th>pusherName</th>
      <th>beforeID</th>
      <th>afterID</th>
      <th>dateTime</th>
      <th></th>
    </tr>

    <% _.each(users, function(user){ %>
    <tr data-id="<%= user.id %>" data-model="user">
      <td><%= user.id %></td>
      <td><%= user.repositoryName %></td>
      <td><%= user.repositoryID%></td>
            <td><%= user.pusherName%></td>

      <td><%= user.beforeID%></td>
      <td><%= user.afterID%></td>
      <td><%= user.dateTime%></td>

      <td><a href="/user/show/<%= user.id %>" class="btn btn-sm btn-primary">Show</a></td>
      <td><a href="/user/edit/<%= user.id %>" class="btn btn-sm btn-warning">Edit</a></td>
      <td><a href="/user/destroy/<%= user.id %>" class="btn btn-sm btn-danger">Delete</a></td>


            <td>
        <form action="/user/destroy/<%= user.id%>" method="POST">
          <input type="hidden" name="_method" value="delete"/>
          <input type="submit" class="btn btn-sm btn-danger" value="Delete"/>
          <input type="hidden" name="_csrf" value="<%= _csrf %>"/>
        </form>
      </td>

    </tr>

    <%}) %>
  </table>
</div>
